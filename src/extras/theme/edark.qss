QWidget {
    border: 0px solid transparent;
}

QMainWindow {
    background-color: #171814;
}

QMainWindow::separator {
    background: #22241e;
    border: 0px solid transparent;
    height: 5px;
}

QMainWindow::separator:hover {
    background: #333;
}

QToolBar {
    padding: 5px;
}

QToolBar::handle {
    border: 1px solid gray;
    border-radius: 1px;
    height: 7px;
    margin: 3px;
}

QToolBar#toolbar {
    background: #171814;
}

QToolBar::separator {
    background: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0,
                                stop:0 #171814, stop: 0.5 #E6E6E6,
                                stop: 0.6 #E6E6E6, stop:1 #171814);
    width: 1px;
    height: 1px;
}

QToolBar#preferencias {
    background: #171814;
    spacing: 7px;
    padding: 7px;
}

QToolButton {
    color: #FFFFFF;
}

QToolButton:hover {
    background: #22241e;
}

QToolButton#dock_button:checked {
    background: #171814;
    border-radius: 2px;
}

QToolButton#dock_button:hover {
    background: #575757;
    border-radius: 2px;
}

QToolButton#combo-lines-button,
QToolButton#combo-button {
    border-style: solid;
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 0.8,
                                stop: 0 #414238, stop: 1 #1e1f1a);
}
QToolButton#combo-lines-button {
    padding: 3px 10px 3px 10px;
}

QToolButton#combo-lines-button:hover,
QToolButton#combo-button:hover {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 0.8,
                                      stop: 0 #484a3f, stop: 1.0 #1e1f1a);
}

QScrollBar:vertical {
    background: transparent;
    width: 12px;
}

QScrollBar:horizontal {
    background: transparent;
    height: 10px;
}

QScrollBar::handle:horizontal {
    border-radius: 1px;
    background: #353535;
    min-width: 50px;
}

QScrollBar::handle:vertical {
    border-radius: 1px;
    background: #353535;
    min-height: 50px;

}


QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical,
QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical,
QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal,
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal{
    background: #171814;
}

QScrollBar::handle:vertical:hover,
QScrollBar::handle:horizontal:hover {
    background: #404040;
}

QMenuBar,
QStatusBar {
    background-color: #171814;
    color: #FFFFFF;
}

QMenuBar::item {
    background: #171814;
    padding: 2px 7px 5px 7px;
    color: #FFFFFF;
}

QMenuBar::item:selected {
    background: #2a2b24;
    border-radius: 1px;
    color: #FFFFFF;
}

QMenuBar::item:pressed {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                      stop: 0 #2e3028, stop: 1.0 #22241e);
    color: #FFFFFF;
}

QMenu {
    background-color: #171814;
    color: #FFFFFF;
    border: 1px solid #333333;
    border-radius: 1px;
    padding-top: 3px;
    padding-left: 2px;
}

QMenu::item {
    padding-top: 3px;
    padding-left: 25px;
    padding-bottom: 3px;
}

QMenu::item:selected {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                      stop: 0 #2e3028, stop: 1.0 #22241e);
}

QMenu::separator {
    background: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0,
                                stop:0 #171814, stop: 0.5 #E6E6E6,
                                stop: 0.6 #E6E6E6, stop:1 #171814);

    height: 1px;
    margin-left: 15px;
    margin-right: 15px;
}

QListView,
QTreeView,
QTreeWidget {
    background: #2d2e27;
    color: #FFFFFF;
}

QDockWidget,
QDockWidget::title {
    background: #171814;
}

QTreeWidget::item:selected:!active {
    background: none;
}

QListWidget#list-selector::item:selected {
    background-color: rgba(77, 82, 110, 75%);
    border: 0px solid transparent;
    border-radius: 3px;
}

QTreeWidget::item:selected,
QListWidget::item:selected,
QTreeView::item:selected {
    background: #4d526e;
    color: #FFFFFF;
    border: 1px solid #FFFFFF;
}

QDialog {
    background-color: #171814;
}

QDialog#file-selector {
    border: 1px solid gray;
    border-radius: 3px;
}

QLabel#status_label {
    color: #FFFFFF;
    background: transparent;
}

QToolButton#combo-label {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 0.8,
                                stop: 0 #474a3e, stop: 1 #171814);

    padding: 4px 10px 4px 10px;
    min-width: 6em;
}

QPushButton {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #22241e, stop: 1 #22241e);
    border-radius: 3px;
    min-width: 120px;
    min-height: 30px;
    color: #dedede;
}

QPushButton:focus {
    border: 1px solid #5f6587;
}

QPushButton#custom {
    min-width: 200px;
}

QPushButton:hover {
    border: 1px solid #606060;
    background: #2a2b24;
}

QPushButton:pressed {
    border: 1px solid gray;
}

QLineEdit {
    background: #22241e;
    border-radius: 1px;
    color: #FFFFFF;
}

QLineEdit#popup {
    border: 1px solid gray;
}

QLabel,
QRadioButton,
QCheckBox {
    background-color: #171814;
    color: #FFFFFF;
}

QCheckBox:disabled {
    color: #474747;
}

QGroupBox {
    color: #FFFFFF;
    margin-top: 20px;
    padding: 5px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    padding: 5 10px;
}

QTabBar {
    background: #171814;
}

QTabWidget::tab-bar:right {
    left: 3px;
}

QTabWidget::tab-bar:left {
    right: 3px;
}

QTabBar::tab {
    color: #FFFFFF;
    background: #1d1f19;
}

QTabBar::tab:top {
    padding: 6px 25px;
}

QTabBar::tab:left,
QTabBar::tab:right {
    padding: 25px 5px;
}

QTabBar::tab:top:selected {
    border-bottom: 3px solid #333;
}

QTabWidget::pane {
    border: 0px solid transparent;
    padding: 3px;
    position: absolute;
}

QTabBar::tab:hover {
    background: #22241e;
}

QSlider::groove:horizontal {
    height: 7px;
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #B1B1B1, stop:1 #c4c4c4);
    margin: 2px 0;
}

QSlider::handle:horizontal {
    background: #555;
    width: 25px;
    margin: -2px 0;
    border-radius: 1px;
}

QSlider::handle:horizontal:hover {
    background: gray;
}

QComboBox {
    background:qlineargradient(x1: 0, y1: 0, x2: 0, y2: 0.8,
                               stop: 0 #414238, stop: 1 #1e1f1a);
    color: #FFFFFF;
    padding: 3px 10px 3px 10px;

}

QComboBox::drop-down {
    border-left-width: 1px;
    width: 5px;
}

QComboBox::down-arrow {
    image: url(:image/arrow-down);
    top: 0px;
    right: 7px;
}

QComboBox:item:selected {
    background: #4d526e;
}

QComboBox:on {
    background: transparent;
}

QComboBox:hover {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 0.8,
                                      stop: 0 #484a3f, stop: 1.0 #1e1f1a);
}

QComboBox QAbstractItemView {
     border: 1px solid darkgray;
     background-color: #303030;
     margin-top: 0px;
     padding-bottom: 0px;
}

QSpinBox {
    background: #DEDEDE;
    border-radius: 3px;
    padding: 1px 4px 3px 2px;
}

QSpinBox::up-button {
    background: #303030;
    subcontrol-origin: border;
    subcontrol-position: top right;
    border-image: url(:/image/arrow-up) 1;
}

QSpinBox::down-button {
    background: #303030;
    border-image: url(:/image/arrow-down)
}

QSpinBox::up-button:hover,
QSpinBox::down-button:hover {
    background: #999;
    border-radius: 2px;
}

QLCDNumber {
    color: #FFFFFF;
}

QHeaderView::section {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #717171, stop: 0.5 #616161,
                                stop: 0.6 #616161, stop:1 #717171);
    padding-left: 5px;
    color: #FFFFFF;
    border: 1px solid #555;
}